{
  "name": "pdf-to-printer",
  "version": "0.1.0",
  "description": "A utility to print PDF files from Node.js apps on Windows and Unix-like platforms",
  "license": "MIT",
  "author": "Artur Khusaenov <artur.khusaenov@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/artiebits/pdf-to-printer.git"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "copy-SumatraPDF.exe": "cp ./src/win32/SumatraPDF.exe ./dist/win32",
    "build": "babel src --out-dir dist --source-maps inline && npm run copy-SumatraPDF.exe",
    "prepublishOnly": "rimraf dist && npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "babel-jest": "24.8.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3"
  },
  "keywords": [
    "pdf",
    "printer",
    "pdf-printer",
    "zebra-printer",
    "rollo-printer",
    "node.js",
    "electron",
    "windows",
    "macos",
    "linux"
  ]
}
