{
  "name": "pdf-to-printer",
  "version": "5.3.0",
  "description": "A utility to print PDF files from Node.js and Electron",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/artiebits/pdf-to-printer.git"
  },
  "main": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "prettier --write \"**/*.{js,ts,json,md,yml}\"",
    "build": "webpack --config webpack.production.config.js",
    "prepare": "npm run lint && npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "babel-jest": "^28.0.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "husky": "^7.0.4",
    "jest": "^27.4.0",
    "jest-mock": "^27.5.1",
    "lint-staged": "^12.1.2",
    "prettier": "2.5.1",
    "ts-jest": "^27.0.6",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0"
  },
  "keywords": [
    "pdf",
    "printer",
    "pdf-printer",
    "zebra-printer",
    "rollo-printer",
    "node.js",
    "electron",
    "windows",
    "macos",
    "linux"
  ]
}
